<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.littlebug.mapper.InterviewQuestionMapper">

    <resultMap id="BaseResultMap" type="com.littlebug.pojo.InterviewQuestion">
            <id property="id" column="id" />
            <result property="interviewId" column="interview_id" />
            <result property="questionIndex" column="question_index" />
            <result property="questionText" column="question_text" />
            <result property="questionType" column="question_type" />
            <result property="difficulty" column="difficulty" />
            <result property="answerDuration" column="answer_duration" />
            <result property="createTime" column="create_time" />
    </resultMap>

    <sql id="Base_Column_List">
        id,interview_id,question_index,question_text,question_type,difficulty,
        answer_duration,create_time
    </sql>
</mapper>
